{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "first_name": {
      "type": "string",
      "maxLength": 50
    },
    "last_name": {
      "type": "string",
      "maxLength": 100
    },
    "company_name": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "maxLength": 20
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "language": {
      "type": "enumeration",
      "required": false,
      "default": "fr",
      "enum": [
        "fr",
        "en"
      ]
    },
    "country": {
      "type": "string",
      "default": "FR",
      "maxLength": 2,
      "required": false
    },
    "email_verified_at": {
      "type": "datetime"
    },
    "statusUser": {
      "type": "enumeration",
      "required": false,
      "default": "pending",
      "enum": [
        "active",
        "pending",
        "inactive",
        "suspended"
      ]
    },
    "last_login_at": {
      "type": "datetime"
    },
    "lastPredashboardSeenAt": {
      "type": "datetime"
    },
    "coached_athletes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::coach-athlete.coach-athlete",
      "mappedBy": "coach"
    },
    "athlete_relations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::coach-athlete.coach-athlete",
      "mappedBy": "athlete"
    },
    "sessions_as_coach": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::session.session",
      "mappedBy": "coach"
    },
    "sessions_as_athlete": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::session.session",
      "mappedBy": "athlete"
    },
    "conversations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::conversation.conversation",
      "mappedBy": "coach_id"
    },
    "conversation_participations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::conversation-participant.conversation-participant",
      "mappedBy": "user"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "sender"
    },
    "platform_subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::platform-subscription.platform-subscription",
      "mappedBy": "coach"
    },
    "pricing_formulas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pricing-formula.pricing-formula",
      "mappedBy": "coach"
    },
    "athlete_subscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::athlete-subscription.athlete-subscription",
      "mappedBy": "athlete"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "user"
    },
    "audit_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::audit-log.audit-log",
      "mappedBy": "user"
    },
    "audit_logs_affected": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::audit-log.audit-log",
      "mappedBy": "affected_user"
    }
  }
}
