{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "singularName": "payment",
    "pluralName": "payments",
    "displayName": "Payment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "athlete_subscription": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::athlete-subscription.athlete-subscription",
      "inversedBy": "payments"
    },
    "amount": {
      "type": "biginteger",
      "required": true
    },
    "payment_method": {
      "type": "string",
      "maxLength": 50
    },
    "stripe_payment_intent": {
      "type": "text"
    },
    "stripe_payment_method": {
      "type": "text"
    },
    "statusPayment": {
      "type": "enumeration",
      "enum": [
        "pending",
        "processing",
        "succeeded",
        "failed",
        "cancelled",
        "refunded"
      ]
    },
    "failure_reason": {
      "type": "text"
    },
    "processed_at": {
      "type": "datetime"
    },
    "refunded_at": {
      "type": "datetime"
    },
    "refund_amount": {
      "type": "biginteger",
      "default": "0"
    },
    "metadata": {
      "type": "json"
    }
  }
}
