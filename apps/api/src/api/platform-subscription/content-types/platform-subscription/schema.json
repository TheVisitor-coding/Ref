{
  "kind": "collectionType",
  "collectionName": "platform_subscriptions",
  "info": {
    "singularName": "platform-subscription",
    "pluralName": "platform-subscriptions",
    "displayName": "Platform_subscription"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "platform_invoices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::platform-invoice.platform-invoice",
      "mappedBy": "subscription"
    },
    "coach": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "platform_subscriptions"
    },
    "platform_plan": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::platform-plan.platform-plan",
      "inversedBy": "platform_subscriptions"
    },
    "billing_cycle": {
      "type": "enumeration",
      "enum": [
        "monthly",
        "yearly"
      ]
    },
    "statusSubscription": {
      "type": "enumeration",
      "enum": [
        "active",
        "past_due",
        "cancelled",
        "unpaid"
      ]
    },
    "current_period_start": {
      "type": "datetime",
      "required": true
    },
    "current_period_end": {
      "type": "datetime",
      "required": true
    },
    "cancel_at_period_end": {
      "type": "boolean",
      "default": false
    },
    "stripe_subscription_id": {
      "type": "string"
    },
    "stripe_customer_id": {
      "type": "string"
    },
    "trial_end": {
      "type": "datetime"
    }
  }
}
